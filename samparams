sort -@ 1 -m 10000M -o ../sorted/outputOfTest/sorted.bam ../unsorted_bam_files/onepercent.bam
sort -@ 2 -m 5000M -o ../sorted/outputOfTest/sorted.bam ../unsorted_bam_files/onepercent.bam
sort -@ 3 -m 3333M -o ../sorted/outputOfTest/sorted.bam ../unsorted_bam_files/onepercent.bam
sort -@ 4 -m 2500M -o ../sorted/outputOfTest/sorted.bam ../unsorted_bam_files/onepercent.bam
view -@ 1 -h -o ../sorted/outputOfTest/sorted.bam ../unsorted_bam_files/onepercent.bam
view -@ 2 -h -o ../sorted/outputOfTest/sorted.bam ../unsorted_bam_files/onepercent.bam
view -@ 3 -h -o ../sorted/outputOfTest/sorted.bam ../unsorted_bam_files/onepercent.bam
view -@ 4 -h -o ../sorted/outputOfTest/sorted.bam ../unsorted_bam_files/onepercent.bam



